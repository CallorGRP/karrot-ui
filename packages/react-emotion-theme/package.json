{
  "name": "@karrotmarket/react-emotion-theme",
  "amdName": "karrotReactEmotionTheme",
  "version": "0.4.0",
  "sideEffects": false,
  "main": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daangn/karrot-ui",
    "directory": "packages/react-emotion-theme"
  },
  "maintainers": [
    {
      "name": "Tim Kim",
      "email": "tim@daangn.com"
    }
  ],
  "publishConfig": {
    "access": "public",
    "source": "./src/index.ts",
    "main": "./lib/index.js",
    "module": "./lib/index.module.js",
    "exports": "./lib/index.modern.js",
    "types": "./lib/index.d.ts"
  },
  "scripts": {
    "build": "microbundle --jsx jsx --jsxImportSource react --globals react/jsx-runtime=jsx,@emotion/react=emotionReact,@karrotmarket/design-token=karrotDesignToken,@karrotmarket/react-theming=karrotReactTheming",
    "dev": "microbundle dev --jsx jsx --jsxImportSource react --globals react/jsx-runtime=jsx,@emotion/react=emotionReact,@karrotmarket/design-token=karrotDesignToken,@karrotmarket/react-theming=karrotReactTheming",
    "prepack": "yarn build"
  },
  "files": [
    "src",
    "lib"
  ],
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "react": "^17.0.0"
  },
  "dependencies": {
    "@karrotmarket/design-token": "workspace:^0.2.0",
    "@karrotmarket/react-theming": "workspace:^0.2.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@emotion/react": "^11.4.1",
    "@types/node": "^16.9.0",
    "@types/react": "^17.0.20",
    "@types/webpack-env": "^1.16.2",
    "microbundle": "^0.13.3",
    "react": "^17.0.2",
    "typescript": "^4.4.2"
  }
}
