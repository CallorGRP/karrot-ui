    "@babel/preset-typescript": "^7.13.0",
    "@types/common-tags": "^1.8.0",
    "@types/tap": "^14.10.3",
    "babel-plugin-module-extension-resolver": "^1.0.0-rc.2",
    "babel-preset-modules": "^0.0.2",
    "common-tags": "^1.8.0",
    "concurrently": "^6.0.0",
    "tap": "^14.11.0",
    "typescript": "^4.2.3"
  }
}
